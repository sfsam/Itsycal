	<false/>
	<key>SUEnableAutomaticChecks</key>
	<true/>
	<key>SUFeedURL</key>
	<string>https://s3.amazonaws.com/itsycal/itsycal.xml</string>
	<key>SUScheduledCheckInterval</key>
	<real>345600</real>
